{
  "name": "agrippa",
  "version": "0.1.0",
  "private": true,
  "module": "dist/bundle.js",
  "main": "src/electron/index.js",
  "homepage": "./",
  "scripts": {
    "start": "yarn server",
    "start:all": "concurrently \"yarn server\" \"wait-on http://localhost:8080 && electron .\"",
    "server": "cross-env NODE_ENV=development nodemon ./src/server/index.js --exec babel-node -w ./src/server/index.js",
    "lint": "eslint ./src --ext .js,.jsx -c .eslintrc.yml",
    "build": "yarn build:server && yarn build:electron",
    "build:electron": "electron-builder",
    "build:server": "cross-env NODE_ENV=production webpack -p",
    "release": "yarn build:server && electron-builder --publish=always"
  },
  "dependencies": {
    "@monaco-editor/react": "^1.2.1",
    "axios": "^0.19.0",
    "classnames": "^2.2.6",
    "cors": "^2.8.5",
    "d3": "^5.9.7",
    "electron-is-dev": "^1.1.0",
    "express": "^4.17.1",
    "lodash": "^4.17.15",
    "morgan": "^1.9.1",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.1.0",
    "react-scripts": "3.0.1",
    "redux": "^4.0.4",
    "semantic-ui-react": "^0.87.2",
    "video-react": "^0.14.1",
    "webpack": "^4.37.0",
    "webpack-dev-middleware": "^3.7.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.4.5",
    "@babel/node": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.4.4",
    "babel-eslint": "^9.0.0",
    "babel-loader": "^8.0.6",
    "babel-preset-minify": "^0.5.0",
    "concurrently": "^4.1.0",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "electron": "^5.0.5",
    "electron-builder": "^20.44.4",
    "electron-chrome-extension": "^3.0.0-rc.3",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.13.0",
    "eslint-stats": "^1.0.1",
    "json-loader": "^0.5.7",
    "monaco-editor": "^0.17.1",
    "node-libs-browser": "^1.1.1",
    "node-sass": "^4.12.0",
    "nodemon": "1.18.10",
    "raw-loader": "^0.5.1",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "wait-on": "^3.2.0",
    "webpack-cli": "^3.3.6"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
